<?xml version="1.0" encoding="utf-8"?>
<RunSettings>
  <RunConfiguration>
    <!-- 0 = As many processes as possible, limited by number of cores on machine, 1 = Sequential -->
    <MaxCpuCount>1</MaxCpuCount>
    <!-- Timeout in milliseconds per test -->
    <TestSessionTimeout>60000</TestSessionTimeout>
  </RunConfiguration>
  
  <NUnit>
    <!-- Where the test results will be saved -->
    <WorkDirectory>.</WorkDirectory>
    
    <!-- Number of test threads -->
    <NumberOfTestWorkers>1</NumberOfTestWorkers>
    
    <!-- Default timeout for each test case in milliseconds -->
    <DefaultTimeout>30000</DefaultTimeout>
    
    <!-- Stop on first failure? -->
    <StopOnError>false</StopOnError>
    
    <!-- Test Output verbosity: Off, Error, Warning, Info, Verbose -->
    <Verbosity>3</Verbosity>
    
    <!-- Display test output in console? -->
    <DisplayTestOutput>true</DisplayTestOutput>
  </NUnit>
  
  <LoggerRunSettings>
    <Loggers>
      <Logger friendlyName="console" enabled="True">
        <Configuration>
          <Verbosity>detailed</Verbosity>
        </Configuration>
      </Logger>
      <Logger friendlyName="trx" enabled="True">
        <Configuration>
          <LogFileName>TestResults.trx</LogFileName>
        </Configuration>
      </Logger>
    </Loggers>
  </LoggerRunSettings>
</RunSettings>

